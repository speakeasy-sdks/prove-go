                        MobileOperatorName:
                            type: string
                            example: "Verizon"
                            description: The carrier related to the phone number.
        ScoreResponse:
            type: object
            properties:
                requestId:
                    type: string
                    example: 7f83-b0c4-90e0-90b3-11e10800200c9a66
                    description: The requestId from the request, reflected back for tracking purposes.
                status:
